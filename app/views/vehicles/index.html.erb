<h1>Listing vehicles</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Case no</th>
      <th>Mv no</th>
      <th>Engine no</th>
      <th>Chasis no</th>
      <th>Company</th>
      <th>Stolen date</th>
      <th>Stolen place</th>
      <th>Remarks</th>
      <th>Cr no</th>
      <th>Image</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehicles.each do |vehicle| %>
      <tr>
        <td><%= vehicle.user_id %></td>
        <td><%= vehicle.case_no %></td>
        <td><%= vehicle.mv_no %></td>
        <td><%= vehicle.engine_no %></td>
        <td><%= vehicle.chasis_no %></td>
        <td><%= vehicle.company %></td>
        <td><%= vehicle.stolen_date %></td>
        <td><%= vehicle.stolen_place %></td>
        <td><%= vehicle.remarks %></td>
        <td><%= vehicle.cr_no %></td>
        <td><%= vehicle.image %></td>
        <td><%= link_to 'Show', vehicle %></td>
        <td><%= link_to 'Edit', edit_vehicle_path(vehicle) %></td>
        <td><%= link_to 'Destroy', vehicle, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehicle', new_vehicle_path %>
